<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".ExperimentSettingsMultipleChoice">


    <TextView
        android:id="@+id/tv_title_mc4"
        android:layout_width="361dp"
        android:layout_height="51dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="112dp"
        android:layout_marginTop="65dp"
        android:text="Multiple Choice Test Parameters"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/preview_button2"
        android:layout_width="335dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="126dp"
        android:layout_marginLeft="126dp"
        android:layout_marginTop="163dp"
        android:onClick="addFalseSymbolsKinestetic"
        android:text="Add Symbols for Kinestetic Stimuli" />

    <Button
        android:id="@+id/preview_button4"
        android:layout_width="337dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="126dp"
        android:layout_marginLeft="126dp"
        android:layout_marginTop="224dp"
        android:onClick="addFalseSymbolsArtificial"
        android:text="Add Symbols for Artificial Stimuli" />

    <Button
        android:id="@+id/preview_button3"
        android:layout_width="138dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="106dp"
        android:layout_marginLeft="106dp"
        android:layout_marginTop="753dp"
        android:onClick="clearPaths"
        android:text="Clear Paths" />

    <TextView
        android:id="@+id/tw_experiment_select_symbols_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="317dp"
        android:layout_marginEnd="380dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="Kinestetic Symbols URI's"
        android:textColor="#000000"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/tw_experiment_select_symbols_title2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="313dp"
        android:layout_marginEnd="79dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="Artificial Symbols URI's"
        android:textColor="#000000"
        android:textSize="14sp" />

    <Button
        android:id="@+id/create_exp_button2"
        android:layout_width="139dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="127dp"
        android:layout_marginBottom="164dp"
        android:background="@drawable/mybutton"
        android:onClick="saveExperiment2"
        android:text="Save" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="296dp"
        android:layout_height="376dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="353dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="296dp"
        android:layout_height="376dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="353dp"
        android:layout_toEndOf="@+id/textView2" />

</RelativeLayout>